{
  "name": "doxify",
  "version": "1.0.0",
  "description": "Build, edit, and deploy stunning documentation",
  "private": true,
  "workspaces": [
    "services/*",
    "shared/*",
    "apps/*"
  ],
  "scripts": {
    "dev": "docker-compose up",
    "dev:build": "docker-compose up --build",
    "down": "docker-compose down",
    "logs": "docker-compose logs -f",
    "clean": "docker-compose down -v"
  },
  "keywords": [
    "documentation",
    "mdx",
    "cms",
    "docs"
  ],
  "author": "Mihir Kanubhai Rabari",
  "license": "MIT"
}
